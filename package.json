{
  "name": "carbon-tutorial",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/carbon-design-system/carbon-tutorial.git"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-tutorial/issues"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@carbon/grid": "^10.10.2",
    "@carbon/icons-react": "^10.11.0",
    "@fusioncharts/charts": "^3.15.1-sr.1",
    "bootstrap": "^4.5.0",
    "carbon-components": "^10.12.0",
    "carbon-components-react": "^7.12.0",
    "carbon-icons": "^7.0.7",
    "d3": "^5.16.0",
    "d3-svg-legend": "^2.25.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "fusioncharts": "^3.15.1-sr.1",
    "fusionmaps": "^3.15.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fusioncharts": "^3.1.2",
    "react-google-charts": "^3.0.15",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "resize-observer-polyfill": "^1.5.1",
    "sunburst-chart": "^1.9.0"
  },
  "devDependencies": {
    "@babel/helper-create-regexp-features-plugin": "^7.8.8",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^7.0.0",
    "prettier": "^1.17.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "description": "This tutorial will guide you in creating a React app with the [Carbon Design System](https://www.carbondesignsystem.com/). Weâ€™ll teach you the ins and outs of using Carbon components, while introducing web development best practices along the way.",
  "homepage": "https://github.com/carbon-design-system/carbon-tutorial#readme",
  "main": "index.js",
  "author": "Anjana"
}
